{
  "name": "@hyperbolic/ai-sdk-provider",
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/src/*.d.ts",
      "import": "./dist/src/*.js",
      "default": "./dist/src/*.js"
    }
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "pnpm with-env tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "pnpm with-env tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path .prettierignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env -c --",
    "codegen:update-models": "pnpm with-env npx tsx src/scripts/update-models-list.ts"
  },
  "prettier": "@hyperbolic/prettier-config",
  "devDependencies": {
    "@hyperbolic/api": "workspace:*",
    "@hyperbolic/eslint-config": "workspace:*",
    "@hyperbolic/prettier-config": "workspace:*",
    "@hyperbolic/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "handlebars": "^4.7.8",
    "prettier": "catalog:",
    "type-fest": "^4.37.0",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@ai-sdk/provider": "^1.0.10",
    "@ai-sdk/provider-utils": "^2.1.12",
    "@openrouter/ai-sdk-provider": "^0.4.3",
    "zod": "^3.24.2"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts"
}
